[
    {
        "col": "col-sm-12",
        "id": "username",
        "type": "text",
        "required": true,
        "regex": "^[A-Za-z0-9\\._]+@(([A-Za-z0-9]+\\.)+[A-Za-z0-9]{2,5})$",
        "placeholder": "username"
    },
    {
        "col": "col-sm-12",
        "id": "password",
        "type": "password",
        "required": true,
        "placeholder": "password",
        "feedback": false
    },
    {
        "col": "col-sm-12",
        "id": "terms",
        "type": "checkbox",
        "required": true,
        "placeholder": "Terms",
        "label": "Confermando accetto <a href=\"https://google.com\">Termini e Condizioni</a>"
    }
]